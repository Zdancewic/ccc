type Bool = One + One

let true : Bool = inl () in
let false : Bool = inr () in
fun (x:Bool) (y:Bool) ->
  begin match x with
  | inl x1 ->
    begin match y with
    | inl y1 -> true
    | inr y1 -> false
    end
  | inr x2 ->
    begin match y with
    | inl y1 -> false
    | inr y1 -> true
    end
 end
  
